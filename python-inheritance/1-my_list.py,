#!/usr/bin/python3
"""
1-my_list.py

This module defines the MyList class, which inherits from the built-in list.
It includes a method to print the list in sorted order.
"""

class MyList(list):
    """A class that inherits from list with an additional method to print sorted."""
    
    def print_sorted(self):
        """Prints the list in ascending sorted order."""
        sorted_list = sorted(self)  # Sort the list in ascending order
        print(sorted_list)
